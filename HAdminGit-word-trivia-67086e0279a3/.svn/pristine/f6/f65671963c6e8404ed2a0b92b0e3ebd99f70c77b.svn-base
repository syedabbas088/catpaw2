#import <UIKit/UIKit.h>

@interface CustomButton : UIButton

@property(nonatomic,assign)BOOL clicked;
@property(nonatomic,assign)BOOL isMoved;
@property(nonatomic,assign)BOOL canBeMoved;
@property(nonatomic,assign)int tagOfHiddenButton;
@property(nonatomic,assign)double myHeight;
@property(nonatomic,assign)double myWidth;
@property(nonatomic,assign)NSString *myText;
@property(nonatomic,assign)NSString *myAlphabet;
@property(nonatomic,assign)NSString *myTitleValue;
@property(nonatomic,assign)BOOL fixedBeforeGameStart;

-(void)MoveUp;
-(void)MoveDown;
-(void)LockIt;
-(void)AnimateJump;
-(void)setAlphabet:(NSString*)letter;
@end
